{
system("Color C");
setlocale(LC_ALL,"Portuguese");
float num1 ,num2;
char op;
cout<< "Entre com numero operador numero: \n";
cin>> num1>>op>>num2;

switch (op){
case '+':
	cout<<" = " << setprecision(2) << num1 + num2;
	break;
case '-':
	cout<< " = "<< setprecision(2) << num1 - num2;
	break;
case '*':
	cout<< "=" << setprecision(2) << num1 * num2;
case'/':
	cout<< " = " << setprecision(2) <<num1 / num2;
	break;
default:
cout<< " Operador invalido.";
break;
}
}
